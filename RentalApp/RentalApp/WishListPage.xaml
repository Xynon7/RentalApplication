<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RentalApp.WishListPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Wish List"/>
            <Label Text="*List of unavaliable items*"/>
            <Label Text="*List of items added to the wish list*"/>
            <Entry Placeholder="Cost Range - High end"/>
            <Entry Placeholder="Cost Range - Low end"/>
            <Entry Placeholder="Brand"/>
            <Entry Placeholder="Type"/>
            <Entry Placeholder="Description Phrase"/>
            <Button Text="Add to Wish List"/>
            <Button Text="Return to Result"/>
            <Button Text="Home"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>